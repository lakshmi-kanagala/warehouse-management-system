package com.drakes.itsfresh.model;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.Table;
import jakarta.persistence.UniqueConstraint;
import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter
@ToString
@Entity
@Table(uniqueConstraints = @UniqueConstraint(columnNames = {"store_id", "prod_no"}))
public class StoreLine {
	@ManyToOne
	@JoinColumn(name="store_id", referencedColumnName = "id")
	private Customer storeId;
	@ManyToOne
	@JoinColumn(name="prod_no", referencedColumnName = "id")
	private Produce prodNo;
	private Integer selectOrBlock;
}

